{
  "name": "carwash-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "build": "yarn prisma generate && yarn tsc",
    "lint": "yarn eslint . --ext .ts --fix",
    "prisma:migrate": "dotenv -e .env_local -- yarn prisma migrate dev",
    "start": "dotenv -e .env_stage ts-node ./src/bin/index",
    "dev": "dotenv -e .env_local -- ts-node-dev -r tsconfig-paths/register ./src/bin/index.ts",
    "seed": "dotenv -e .env_test -- ts-node ./src/helpers/databaseHelpers/seed.ts --exit",
    "seed:local": "dotenv -e .env_local -- ts-node ./src/helpers/databaseHelpers/seed.ts --exit",
    "test": "dotenv -e .env_test -- jest --silent",
    "test:all": "yarn test:integration && yarn test",
    "deploy:docker": "dotenv -e .env_test -- npx prisma db push",
    "jest:integration": "dotenv -e .env_test -- yarn seed && dotenv -e .env_test -- jest --config jest.integration.config.js --testTimeout=150000 --silent --runInBand",
    "test:integration": "yarn docker:up && yarn deploy:docker && yarn jest:integration && yarn docker:down"
  },
  "engines": {
    "yarn": "1.22.19"
  },
  "dependencies": {
    "@prisma/client": "4.11.0",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/socket.io": "^3.0.2",
    "@types/yup": "^0.29.13",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "node-cron": "^3.0.1",
    "node-schedule": "^2.1.0",
    "prisma": "4.11.0",
    "socket.io": "4.6.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.4",
    "@types/node": "^16.4.10",
    "@types/node-schedule": "^2.1.0",
    "@types/randomstring": "^1.1.8",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv-cli": "^6.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.5.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "node-mocks-http": "^1.12.1",
    "parse-database-url": "^0.3.0",
    "prettier": "^2.8.0",
    "randomstring": "^1.2.3",
    "socket.io-client": "^4.6.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "tsconfig-paths": "^4.1.1",
    "yarn": "1.22.19"
  }
}
