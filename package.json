{
  "name": "carwash-api",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "build": "tsc && tsc-alias",
    "build:all": "yarn build:swagger && yarn build",
    "lint": "yarn eslint . --ext .ts --fix",
    "prisma:migrate": "dotenv -e .env_local -- yarn prisma migrate dev",
    "start": "node dist/bin/index.js",
    "dev": "dotenv -e .env -- tsx watch ./src/bin/index.ts",
    "seed": "tsx ./src/helpers/databaseHelpers/seed.ts --exit",
    "seed:local": "dotenv -e .env_local -- tsx ./src/helpers/databaseHelpers/seed.ts --exit",
    "test": "dotenv -e .env_test -- jest --verbose",
    "test:all": "yarn test:integration:docker && yarn test",
    "push:seed:local": "dotenv -e .env_local -- npx prisma db push && dotenv -e .env_local -- yarn seed",
    "push:seed": "dotenv -e .env_test -- npx prisma db push && dotenv -e .env_test -- yarn seed",
    "test:intg": "dotenv -e .env_test -- node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.integration.config.js --testTimeout=150000 --verbose --silent --runInBand",
    "test:intg:no-docker": "yarn push:seed && dotenv -e .env_test -- yarn test:intg",
    "test:intg:docker": "yarn docker:up && yarn test:intg:no-docker && yarn docker:down",
    "pre-commit": "yarn prettier **/*.ts --fix --write && node ./scripts/checkHardCoded.js",
    "build:swagger": "node ./scripts/build-swagger.js",
    "cleanup:production": "node ./scripts/production-cleanup-simple.cjs",
    "validate:production": "node ./scripts/validate-production.cjs",
    "prepare:production": "yarn cleanup:production && yarn validate:production"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "@aws-sdk/s3-request-presigner": "^3.600.0",
    "@prisma/client": "^6.13.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@types/lodash.debounce": "^4.0.9",
    "@types/nodemailer": "^6.4.17",
    "aws-sdk": "^2.1431.0",
    "axios": "^1.11.0",
    "bcrypt": "^5.1.1",
    "chalk": "4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "csrf-csrf": "^3.0.3",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "expo-server-sdk": "^3.7.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.4.1",
    "firebase-admin": "^13.4.0",
    "helmet": "^6.0.1",
    "ioredis": "^5.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash.debounce": "^4.0.8",
    "mime-db": "^1.52.0",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^5.1.5",
    "node-cron": "^3.0.1",
    "node-schedule": "^2.1.0",
    "nodemailer": "^7.0.5",
    "openai": "^5.12.2",
    "prisma": "^6.13.0",
    "rate-limit-redis": "^4.2.0",
    "redis": "^4.7.0",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "uuid": "^10.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.4",
    "@types/mime-db": "^1.43.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.4.10",
    "@types/node-schedule": "^2.1.0",
    "@types/randomstring": "^1.1.8",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^9.0.0",
    "@types/winston": "^2.4.4",
    "@types/yup": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "aws-sdk-client-mock": "^3.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv-cli": "^7.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.5.0",
    "husky": "^8.0.2",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.1",
    "node-mocks-http": "^1.12.1",
    "parse-database-url": "^0.3.0",
    "prettier": "^2.8.0",
    "randomstring": "^1.3.0",
    "socket.io-client": "^4.6.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.5",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths-jest": "^0.0.1",
    "tsx": "^4.19.2",
    "yarn": "1.22.19"
  }
}
