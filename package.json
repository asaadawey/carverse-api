{
  "name": "carwash-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "build": "yarn prisma generate && yarn tsc",
    "lint": "yarn eslint . --ext .ts --fix",
    "prisma:migrate": "dotenv -e .env_local -- yarn prisma migrate dev",
    "start": "ts-node ./src/bin/index",
    "dev": "dotenv -e .env -- ts-node-dev -r tsconfig-paths/register ./src/bin/index.ts",
    "seed": "ts-node ./src/helpers/databaseHelpers/seed.ts --exit",
    "seed:local": "dotenv -e .env_local -- ts-node ./src/helpers/databaseHelpers/seed.ts --exit",
    "test": "dotenv -e .env_test -- jest",
    "test:all": "yarn test:integration:docker && yarn test",
    "push:seed": "dotenv -e .env_test -- npx prisma db push && dotenv -e .env_test -- yarn seed",
    "test:intg": "dotenv -e .env_test -- jest --config jest.integration.config.js --testTimeout=150000 --silent --runInBand",
    "jest:integration": "yarn push:seed && dotenv -e .env_test -- yarn test:intg",
    "test:integration:docker": "yarn docker:up && yarn jest:integration && yarn docker:down",
    "pre-commit": "node ./scripts/checkHardCoded.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "@aws-sdk/s3-request-presigner": "^3.600.0",
    "@prisma/client": "^5.16.1",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/socket.io": "^3.0.2",
    "@types/yup": "^0.29.13",
    "aws-sdk": "^2.1431.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "csrf-csrf": "^3.0.3",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "expo-server-sdk": "^3.7.0",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^8.5.1",
    "mime-db": "^1.52.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.1",
    "node-schedule": "^2.1.0",
    "prisma": "^5.16.1",
    "socket.io": "4.6.2",
    "stripe": "^12.18.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "uuid": "^10.0.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.2.4",
    "@types/mime-db": "^1.43.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.4.10",
    "@types/node-schedule": "^2.1.0",
    "@types/randomstring": "^1.1.8",
    "@types/socket.io-client": "^3.0.0",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "aws-sdk-client-mock": "^3.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "dotenv-cli": "^7.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-security": "^1.5.0",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "jest-mock-extended": "^3.0.1",
    "node-mocks-http": "^1.12.1",
    "parse-database-url": "^0.3.0",
    "prettier": "^2.8.0",
    "randomstring": "^1.3.0",
    "socket.io-client": "^4.6.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "yarn": "1.22.19"
  }
}
